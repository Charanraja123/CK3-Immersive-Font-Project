# Each font should specify what it is used for.
# Name fonts after intented usage, not after source file names.

font = {
	name = "Debug"

	fontstyle = {
		style = regular
		fontfiles = "OpenSans-SemiBold"
	}
}

## Font used in-game
font = {
	name = "StandardGameFont"

	fontstyle = {
		style = regular
		fontfiles = "GitanLatin-RegularIFP"
	}

	fontstyle = {
		style = bold
		fontfiles = "GitanLatin-BoldIFP"
	}

	fontstyle = {
		style = italic
		fontfiles = "GitanLatin-ItalicIFP"
	}

	fontstyle = {
		style = bold|italic
		fontfiles = "GitanLatin-BoldItalicIFP"
	}

	underlineformats = {
		default = {
			thickness = 1
			offset = 0.12
		}
	}
}

# Title font
font = {
	name = "TitleFont"

	fontstyle = {
		style = regular
		fontfiles = "EBGaramondSC08-Regular"
	}
}

## For map names
